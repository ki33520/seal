    "mime": "^1.3.4",
    "mocha": "^2.3.3",
    "moment": "^2.10.6",
    "ms": "^0.7.1",
    "nock": "^2.17.0",
    "nodemon": "^1.3.7",
    "object-assign": "^4.0.1",
    "on-finished": "^2.3.0",
    "range-parser": "^1.0.3",
    "react-hot-loader": "^1.3.0",
    "react-transform-hmr": "^1.0.1",
    "statuses": "^1.2.1",
    "style-loader": "^0.12.3",
    "stylus": "^0.53.0",
    "stylus-loader": "^1.4.2",
    "unpipe": "^1.0.0",
    "webpack": "^1.12.15",
    "webpack-core": "^0.6.8",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.5.0",
