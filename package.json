{
  "name": "seal",
  "version": "0.0.1",
  "description": "isomorphic webapp boilerplate",
  "main": "app.js",
  "scripts": {
    "test-client": "jest",
    "test": "mocha --reporter spec --bail --watch --check-leaks --compilers js:babel-core/register server/__test__/ client/__test__/",
    "start": "NODE_ENV=develop gulp nodemon",
    "start-static": "node task/server.js",
    "develop-static": "npm run static-build && gulp develop-webpack",
    "release-static": "npm run static-prod && gulp deploy-webpack",
    "static-build": "webpack --config ./task/webpack.develop.js --display-error-details",
    "static-prod": "webpack --config ./task/webpack.production.js --display-error-details",
    "vendor-css": "gulp vendor-css"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/task/preprocessor.js",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "testDirectoryName": "__test__",
    "testPathDirs": [
      "<rootDir>/client"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/reflux",
      "<rootDir>/node_modules/bluebird"
    ]
  },
  "keywords": [
    "isomorphic boilerplate"
  ],
  "author": "alichen",
  "repository": "https://github.com/ali322/isomorphic-boilerplate",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^5.8.22",
    "babel-plugin-transform-object-rest-spread": "^6.0.14",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-runtime": "^6.0.14",
    "body-parser": "^1.13.2",
    "classnames": "^2.2.0",
    "consolidate": "^0.13.1",
    "express": "^4.13.1",
    "express-session": "^1.12.1",
    "lodash": "^3.10.1",
    "method-override": "^2.3.3",
    "normalize.css": "^3.0.3",
    "react": "^0.14.2",
    "react-dom": "^0.14.2",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.0.0",
    "redux": "^3.0.4",
    "redux-logger": "^2.0.4",
    "redux-thunk": "^1.0.0",
    "reqwest": "^2.0.5",
    "swig": "^1.4.2",
    "xhr2": "^0.1.3"
  },
  "devDependencies": {
    "autoprefixer-loader": "^2.0.0",
    "babel-loader": "^5.3.1",
    "css-loader": "^0.14.5",
    "del": "^1.2.0",
    "expect": "^1.12.2",
    "extract-text-webpack-plugin": "^0.8.2",
    "gulp": "^3.9.0",
    "gulp-concat": "^2.6.0",
    "gulp-inject": "^1.3.1",
    "gulp-livereload": "^3.8.0",
    "gulp-minify-css": "^1.2.1",
    "gulp-rename": "^1.2.2",
    "jest-cli": "^0.7.0",
    "methods": "^1.1.1",
    "mocha": "^2.3.3",
    "nock": "^2.17.0",
    "nodemon": "^1.3.7",
    "react-hot-loader": "^1.2.8",
    "sass-loader": "^1.0.2",
    "style-loader": "^0.12.3",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.10.1"
  }
}
