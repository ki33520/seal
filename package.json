{
    "name": "seal",
    "version": "0.0.1",
    "description": "isomorphic webapp boilerplate",
    "main": "app.js",
    "scripts": {
        "start": "node app.js",
        "test": "npm run backend-test && npm run frontend-test",
        "frontend-test": "FRONTEND_TEST_PORT=8000 karma start ./task/karma.config.js",
        "backend-test": "mocha --reporter list --bail --check-leaks --compilers es6:babel-core/register server/__tests__/",
        "develop": "gulp start",
        "start-static": "node task/static-server.js",
        "develop-static": "npm run static-build && gulp develop-webpack",
        "release-static": "npm run static-prod && gulp deploy-webpack",
        "static-build": "webpack --config ./task/webpack.develop.js --display-error-details",
        "static-prod": "webpack --config ./task/webpack.production.js --display-error-details",
        "vendor-css": "gulp vendor-css"
    },
    "keywords": [
        "isomorphic boilerplate"
    ],
    "author": "alichen",
    "repository": "https://github.com/ali322/isomorphic-boilerplate",
    "license": "MIT",
    "dependencies": {
        "babel-core": "^5.8.22",
        "babel-runtime": "^6.0.14",
        "body-parser": "^1.13.2",
        "classnames": "^2.2.0",
        "consolidate": "^0.13.1",
        "express": "^4.13.1",
        "express-session": "^1.12.1",
        "lodash": "^3.10.1",
        "method-override": "^2.3.3",
        "normalize.css": "^3.0.3",
        "react": "^0.14.2",
        "react-dom": "^0.14.2",
        "react-hot-loader": "^1.3.0",
        "react-redux": "^4.0.0",
        "redux": "^3.0.4",
        "redux-logger": "^2.0.4",
        "redux-thunk": "^1.0.0",
        "reqwest": "^2.0.5",
        "swig": "^1.4.2",
        "xhr2": "^0.1.3"
    },
    "devDependencies": {
        "autoprefixer-loader": "^2.0.0",
        "babel-loader": "^5.3.1",
        "babel-plugin-react-transform": "^1.1.1",
        "bluebird": "^3.0.5",
        "browser-sync": "^2.9.12",
        "css-loader": "^0.14.5",
        "del": "^1.2.0",
        "expect": "^1.12.2",
        "extract-text-webpack-plugin": "^0.8.2",
        "gulp": "^3.9.0",
        "gulp-concat": "^2.6.0",
        "gulp-inject": "^3.0.0",
        "gulp-livereload": "^3.8.0",
        "gulp-minify-css": "^1.2.1",
        "gulp-rename": "^1.2.2",
        "methods": "^1.1.1",
        "mocha": "^2.3.3",
        "nock": "^2.17.0",
        "nodemon": "^1.3.7",
        "react-transform-hmr": "^1.0.1",
        "style-loader": "^0.12.3",
        "stylus-loader": "^1.4.2",
        "webpack": "^1.12.2",
        "webpack-core": "^0.6.8",
        "webpack-dev-middleware": "^1.2.0",
        "webpack-hot-middleware": "^2.5.0"
    }
}
