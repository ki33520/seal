{
  "name": "seal",
  "version": "0.0.1",
  "description": "isomorphic webapp boilerplate",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "linter": "esw -w --ext .js,.jsx,.es6 .",
    "test": "npm run backend-test && npm run frontend-test",
    "frontend-test": "FRONTEND_TEST_PORT=8000 karma start ./task/karma.config.js",
    "backend-test": "mocha --reporter list --bail --check-leaks --compilers es6:babel-core/register server/__tests__/",
    "develop": "NODE_ENV=hotfix gulp start",
    "start-static": "node task/static-server.js",
    "develop-static": "npm run static-build && gulp develop-webpack",
    "release-static": "npm run static-prod && gulp deploy-webpack",
    "static-build": "webpack --config ./task/webpack.develop.js --display-error-details",
    "static-prod": "webpack --config ./task/webpack.production.js --display-error-details",
    "vendor-css": "gulp vendor-css"
  },
  "keywords": [
    "isomorphic boilerplate"
  ],
  "author": "alichen",
  "repository": "https://github.com/ali322/isomorphic-boilerplate",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^5.8.22",
    "babel-runtime": "^6.0.14",
    "body-parser": "^1.13.2",
    "cache-manager": "^2.0.0",
    "cache-manager-fs": "^1.0.4",
    "classnames": "^2.2.0",
    "connect-memcached": "^0.1.0",
    "consolidate": "^0.13.1",
    "cookie-parser": "^1.4.1",
    "express": "^4.13.1",
    "express-session": "^1.12.1",
    "fs-extra": "^0.26.5",
    "immutable": "^3.7.6",
    "jshashes": "^1.0.5",
    "lodash": "^3.10.1",
    "md5": "^2.0.0",
    "memory-cache": "^0.1.4",
    "method-override": "^2.3.3",
    "moment": "^2.10.6",
    "normalize.css": "^3.0.3",
    "object-assign": "^4.0.1",
    "react": "^0.14.8",
    "react-dom": "^0.14.8",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.0.0",
    "redux": "^3.0.4",
    "redux-logger": "^2.0.4",
    "redux-thunk": "^1.0.0",
    "reqwest": "^2.0.5",
    "swig": "^1.4.2",
    "xhr2": "^0.1.3",
    "ya-ui": "^0.2.5"
  },
  "devDependencies": {
    "autoprefixer-loader": "^2.0.0",
    "babel-loader": "^5.3.1",
    "babel-plugin-react-transform": "^1.1.1",
    "bluebird": "^3.0.5",
    "browser-sync": "^2.9.12",
    "cookie-parser": "^1.4.1",
    "css-loader": "^0.14.5",
    "debug": "^2.2.0",
    "del": "^1.2.0",
    "depd": "^1.1.0",
    "destroy": "^1.0.4",
    "escape-html": "^1.0.3",
    "eslint": "^1.10.2",
    "eslint-watch": "^2.1.4",
    "etag": "^1.7.0",
    "expect": "^1.12.2",
    "extract-text-webpack-plugin": "^0.8.2",
    "faker": "^3.0.1",
    "fresh": "^0.3.0",
    "gulp": "^3.9.0",
    "gulp-concat": "^2.6.0",
    "gulp-eslint": "^1.1.1",
    "gulp-inject": "^3.0.0",
    "gulp-inject-string": "^1.1.0",
    "gulp-livereload": "^3.8.0",
    "gulp-minify-css": "^1.2.1",
    "gulp-rename": "^1.2.2",
