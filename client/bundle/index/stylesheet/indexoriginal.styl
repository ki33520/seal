}
.indexSingle span.singlePrice{
    float:right;
    padding-top:rem4px(0.2 * 37.5);
}
.indexSingle span.singlePrice .nowPrice{
    display:inline-block;
    color:#ed1946;
    font-size:rem4px(0.48 * 37.5);
    line-height:rem4px(0.6 * 37.5);
    float:none;
}
.indexSingle span.singlePrice .oldPrice{
    display:inline-block;
    color:#999;
    text-decoration: line-through;
    padding-left:rem4px(0.4 * 37.5);
    line-height:rem4px(0.6 * 37.5);
    float:none;
    font-size:rem4px(0.32 * 37.5);
}

.indexSingle span.singlePrice .nowPrice:first-letter{
    font-size:rem4px(0.4 * 37.5);
    padding-right:rem4px(0.06 * 37.5);
}

