/* flashBuy */
.flashBuy{
    padding:rem4px(0.266 * 37.5) rem4px(0.32 * 37.5) 0;
    border-bottom:rem4px(0.2666 * 37.5) solid #f7f7f7;
}
.flashBuy .title{
    line-height:rem4px(1 * 37.5);
    padding-bottom:rem4px(0.1 * 37.5);
}
.flashBuy .title span i{
    width:rem4px(0.3 * 37.5);
    height:rem4px(0.3 * 37.5);
    display:inline-block;
    background-color:#d9b766;
    margin:0 rem4px(0.2 * 37.5);
}
.flashBuy .title a{
    float:right;
    line-height:rem4px(1 * 37.5);
}
.flashBuy .title a i {
    width: rem4px(0.4 * 37.5);
    height: rem4px(0.4 * 37.5);
    display: inline-block;
    margin: 0 0 0 rem4px(0.1 * 37.5);
    vertical-align: top;
    padding-top: rem4px(0.29 * 37.5);
}
.flashBuy .title a img {
    width: rem4px(0.4 * 37.5);
    height: rem4px(0.4 * 37.5);
    display: block;
    vertical-align: top;
}
.flashBuy > a{
    width:100%;
    position:relative;
    display:block;
    margin-bottom:rem4px(0.266 * 37.5);
}
.flashBuy > a > img{
    width:100%;
}
.flashBuy > a .right{   
    width:rem4px(4.9 * 37.5);
    position:absolute;
    z-index:0;
    right:rem4px(.24 * 37.5);
    top:rem4px(0.75 * 37.5);
}
.flashBuy .right p{
    font-size:rem4px(0.24 * 37.5);
    letter-spacing:-rem4px(0.01 * 37.5);
}
.flashBuy .right p em{
    margin-left:rem4px(0.1 * 37.5);
}
.flashBuy .right p em i{
    color:#ed1946;
}
.flashBuy .right .flashDot{
    width:100%;
    height:rem4px(0.04 * 37.5);
    background-image:url(/client/asset/images/flashDot.png);
    background-size:100%;
    margin:rem4px(0.2 * 37.5) 0 rem4px(0.5 * 37.5);
}
.flashBuy .right span.name{
    font-size:rem4px(0.32 * 37.5);
    height:rem4px(1 * 37.5);
    line-height:rem4px(0.5 * 37.5);
    overflow:hidden;
    display:block;
}
.flashBuy .right span.country{
    display:block;
    margin:rem4px(0.133333 * 37.5) 0 rem4px(0.4 * 37.5);
    line-height:rem4px(0.54 * 37.5);
}
.flashBuy .right span i{
    width: rem4px(0.46 * 37.5);
    height: rem4px(0.46 * 37.5);
    display: inline-block;
    margin-right: rem4px(0.2 * 37.5);
    vertical-align: top;
}
.flashBuy .right span img{
    width: rem4px(0.46 * 37.5);
    height: rem4px(0.46 * 37.5);
    display: inline-block;
    margin-right: rem4px(0.2 * 37.5);
    vertical-align: top;
}
.flashBuy .right .nowPrice{
    float:left;
    color:#ed1946;
    font-size:rem4px(0.48 * 37.5);
    line-height:rem4px(0.6 * 37.5);
    display:block;
    font-weight:bold;
}
.flashBuy .right .oldPrice{
    text-decoration: line-through;
    float:right;
    color:#999;
    display:inline-block;
    line-height:rem4px(0.6 * 37.5);
    font-size:rem4px(0.32 * 37.5);
}
.flashBuy .soldOut{
    width:rem4px(1.813 * 37.5);
    height:rem4px(1.813 * 37.5);
    background-image:url(/client/asset/images/ico_soldOut.png);
    background-size:100%;
    position:absolute;
    left:rem4px(1 * 37.5);
    top:rem4px(2 * 37.5);
}
.flashBuy .right .nowPrice:first-letter{
    font-size:rem4px(.32 * 37.5);
    font-weight:normal;
    padding-right:rem4px(0.06 * 37.5);
}

/* indexSingle */
.indexSingle{
    padding:rem4px(0.266 * 37.5) rem4px(0.32 * 37.5) 0;
