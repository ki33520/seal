@charset "utf-8";
/* CSS Document */
header{
    width:100%;
    height:rem4px(1.15 * 37.5);
    background:#fff url(/client/asset/images/headerBg.png) 0 bottom no-repeat;
    background-size:100%;
    text-align:center;
}
header .title{
    font-size:rem4px(0.46 * 37.5);
    line-height:rem4px(1.15 * 37.5);
    display:block;
    text-align:center;
}
header .logo{
    text-align:center;
}
header .logo img{
    width:rem4px(2.1 * 37.5);
    display: inline-block;
    margin-top: rem4px(.35 * 37.5);
    overflow: hidden;
}
header .backArrow {
    width: rem4px(1.15 * 37.5);
    height: rem4px(1.15 * 37.5);
    line-height: 100%;
    position: absolute;
    top: 0;
    left: 0;
}
header .backArrow:before{
    content: "";
    width:rem4px(0.586667 * 37.5);
    height:rem4px(0.586667 * 37.5);
    display:block;
    background:url(/client/asset/images/backArrow.png) no-repeat;
    background-size:100%;
    margin:rem4px(0.25 * 37.5) 0 0 rem4px(0.3 * 37.5);
}
header .indexQrcode{
    width:rem4px(1.15 * 37.5);
    height:rem4px(1.15 * 37.5);
    line-height:100%;
    position:absolute;
    top:0;
    left:0;
}
header .indexQrcode:before{
    content: "";
    width:rem4px(0.56 * 37.5);
    height:rem4px(0.56 * 37.5);
    display:block;
    background:url(/client/asset/images/indexQrcode.png) no-repeat;
    background-size:100%;
    margin:rem4px(0.32 * 37.5) 0 0 rem4px(0.32 * 37.5);
}
header .indexSearch{
    position:absolute;
    top:0;
    right:0;
    width:rem4px(1.15 * 37.5);
    height:rem4px(1.15 * 37.5);
    text-align:right;
}
header .indexSearch:before{
    content: "";
    width:rem4px(0.614 * 37.5);
    height:rem4px(0.614 * 37.5);
    display:block;
    background:url(/client/asset/images/indexSearch.png) no-repeat;
    background-size:100%;
    margin:rem4px(.32 * 37.5) rem4px(.32 * 37.5) 0 0;
}

#box{
    width:100%;
    overflow-x:hidden;
}
#page-content{
    -webkit-transition:all ease 0.2s;
    transition:all ease 0.2s;
    height:auto;
    width:100%;
}
#page-content > div{
    padding-bottom:rem4px(1.38 * 37.5);
}
.modular {
    width: 100%;
    position:absolute;
}
.title span{
    font-size:rem4px(0.4 * 37.5);
}

/* m-entry */
.m-entry {
  padding:rem4px(0.44 * 37.5) rem4px(.5 * 37.5);
  overflow: hidden;
  border-bottom:rem4px(0.2666 * 37.5) solid #f7f7f7;
  
}
.m-entry span {
  float: left;
  width: 25%;
  font-size: rem4px(0.266667 * 37.5);
  text-align: center;
  line-height: 1;
}
.m-entry span:before {
  content: "";
  display: block;
  width: 40px;
  height: 40px;
  margin: 0 auto;
  background: url(/client/asset/images/enter-ico.png) no-repeat;
  background-size: 100%;
  margin-bottom:rem4px(0.373 * 37.5);
}
.m-entry a:nth-child(2) >span:before {
    background-position:0 -60px;
    background-size: 100%;
}
.m-entry a:nth-child(3) >span:before {
    background-position:0 -120px;
    background-size: 100%;
}
.m-entry a:nth-child(4) >span:before {
    background-position:0 -180px;
    background-size: 100%;
}

.panic{
    padding:rem4px(0.266 * 37.5) rem4px(.32 * 37.5) 0;
    border-bottom:rem4px(0.2666 * 37.5) solid #f7f7f7;
}
.panic a{
    width:100%;
    display:block;
    padding-bottom:rem4px(0.266 * 37.5);
    position:relative;
}
.panic a>img{
    width:100%;
}
.panic a>span{
    width:100%;
    height:rem4px(0.614 * 37.5);
    line-height:rem4px(0.614 * 37.5);
    background-color:rgba(0,0,0,.8);
    color:#fff;
    position:absolute;
    top:0;
    left:0;
    text-align:center;
    font-size:rem4px(0.32 * 37.5);
    vertical-align:middle;
}
.panic a>span i{
    vertical-align:middle;
    display:inline-block;
    height:rem4px(0.614 * 37.5);
    padding-top:rem4px(0.08 * 37.5);
}
.panic a>span img{
    display:inline;
    width:rem4px(0.38 * 37.5);
    height:rem4px(0.38 * 37.5);
    margin-right:rem4px(.1 * 37.5);
}

nav.bottomNav{
    width:100%;
    height:rem4px(1.38 * 37.5);
    overflow:hidden;
    overflow-x:auto;
    border-top:1px solid #d7d7d7;
    position:fixed;
    bottom:0;
    left:0;
    background-color:#fff;
    z-index:990;
}
nav.bottomNav ul{
    height:rem4px(1.38 * 37.5);
}
nav.bottomNav ul li{
    float:left;
    padding:rem4px(0.18 * 37.5) 0 rem4px(0.05 * 37.5);
    width:20%;
}
nav.bottomNav ul li a{
    display:block;
    text-align:center;
    font-size:rem4px(0.27 * 37.5);
}
nav.bottomNav ul li i{
    width:26px;
    height:21px;
    display:block;
    margin:0 auto;
    background-image:url(/client/asset/images/botNav.png);
}
nav.bottomNav ul li:nth-child(1) i{
    background-position:0 0;
    background-size:100%;
}
nav.bottomNav ul li:nth-child(1) a.nav_hover i{
    background-position:0 -21px;
    background-size:100%;
}
nav.bottomNav ul li:nth-child(2) i{
    background-position:0 -42px;
    background-size:100%;
}
nav.bottomNav ul li:nth-child(2) a.nav_hover i{
    background-position:0 -62px;
    background-size:100%;
}
nav.bottomNav ul li:nth-child(3) i{
    background-position:0 -83px;
    background-size:100%;
}
nav.bottomNav ul li:nth-child(3) a.nav_hover i{
    background-position:0 -104px;
    background-size:100%;
}
nav.bottomNav ul li:nth-child(4) i{
    background-position:0 -125px;
    background-size:100%;
}
nav.bottomNav ul li:nth-child(4) a.nav_hover i{
    background-position:0 -146px;
    background-size:100%;
}
nav.bottomNav ul li:nth-child(5) i{
    background-position:0 -168px;
    background-size:100%;
}
nav.bottomNav ul li:nth-child(5) a.nav_hover i{
    background-position:0 -189px;
    background-size:100%;
}
nav.bottomNav ul li a i{
    margin-bottom:rem4px(.1 * 37.5);
}
nav.bottomNav ul li a.nav_hover{
    color:#801da1;
}

.activity{
    padding:rem4px(0.266 * 37.5) rem4px(0.32 * 37.5);
    border-bottom:rem4px(0.2666 * 37.5) solid #f7f7f7;
}
.activity a{
    display:block;
    position:relative;
}
.activity a img{
    width:100%;
}
.activity a span{
    position:absolute;
    left:rem4px(0.133333 * 37.5);
    top:0;
    width:rem4px(0.827 * 37.5);
    height:rem4px(0.827 * 37.5);
    display:block;
}
.activity ul{
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
}
.activity li:nth-child(1){
    width:rem4px(4.027 * 37.5);
}
.activity li:nth-child(2){
    width:rem4px(5.2 * 37.5);
    text-align:right;
    margin-left:rem4px(0.133333 * 37.5);
}
.activity li:nth-child(2) a{
    float:right;
}
.activity li:nth-child(2) a:nth-child(2){
    margin-top:rem4px(0.133334 * 37.5);
}
.activity li:nth-child(1) a span{
    background-image:url(/client/asset/images/indexTitle_1.png);
    background-size:100%;
}
.activity li:nth-child(2) a span{
    background-image:url(/client/asset/images/indexTitle_2.png);
    background-size:100%;
}
.activity li:nth-child(2) a:nth-child(2) span{
    background-image:url(/client/asset/images/indexTitle_3.png);
    background-size:100%;
}

