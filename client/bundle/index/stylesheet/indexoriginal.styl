}
.indexSingle a{
    width:100%;
    display:block;
    padding:rem4px(0.266 * 37.5) 0;
}
.indexSingle a:nth-child(2){
    padding-top:0;
    border-bottom:1px solid #d7d7d7;
}
.indexSingle a > img{
