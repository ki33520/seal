.moveUp-enter {
    position: absolute;
    width: 100%;
    transition-duration: .3s;
    transition-property: transform, opacity;
    transition-timing-function: ease-out;
    transform: translate3d(0, 100%, 0);
    z-index: 10000;
}

.moveUp-enter.moveUp-enter-active {
    transform: translate3d(0, 0, 0);
}

.moveUp-leave {
    position: absolute;
    width: 100%;
    transition-duration: .1s;
    transition-property: transform, opacity;
    transition-timing-function: ease-out;
    transform: translate3d(0, 0, 0);
    opacity: 0;
}

.moveUp-leave.moveUp-leave-active {
    transform: translate3d(0, -15%, 0);
    opacity: 0.3;
}

.moveDown-enter {
    position: absolute;
    width: 100%;
    transition-duration: .3s;
    transition-property: transform, opacity;
    transition-timing-function: ease-out;
    transform: translate3d(0, -100%, 0);
    z-index: 10000;
}

.moveDown-enter.moveDown-enter-active {
    transform: translate3d(0, 0, 0);
}

.moveDown-leave {
    position: absolute;
    width: 100%;
    transition-duration: .1s;
    transition-property: transform, opacity;
    transition-timing-function: ease-out;
    transform: translate3d(0, 0, 0);
    opacity: 0;
}

.moveDown-leave.moveDown-leave-active {
    transform: translate3d(0, -15%, 0);
    opacity: 0.3;
}

.moveLeft-enter {
    position: absolute;
    width: 100%;
    visibility:hidden;
    transition-duration: .3s;
    transition-property: transform, opacity;
    transition-timing-function: ease-out;
    transform: translate3d(-100%, 0, 0);
    z-index: 10000;
}

.moveLeft-enter.moveLeft-enter-active {
    visibility:visible;
    transform: translate3d(0, 0, 0);
}

.moveLeft-leave {
    position: absolute;
    width: 100%;
    transition-duration: .1s;
    transition-property: transform, opacity;
    transition-timing-function: ease-out;
    transform: translate3d(0, 0, 0);
    opacity: 1;
}

.moveLeft-leave.moveLeft-leave-active {
    transform: translate3d(15%, 0, 0);
    opacity: 0.3;
}

.moveRight-enter {
    position: absolute;
    visibility:hidden;
    width: 100%;
    transition-duration: .3s;
    transition-delay:.3s;
    transition-property: transform, opacity;
    transition-timing-function: ease-out;
    transform: translate3d(100%, 0, 0);
    z-index: 10000;
}

.moveRight-enter.moveRight-enter-active {
    visibility:visible;
    transform: translate3d(0, 0, 0);
}

.moveRight-leave {
    position: absolute;
    width: 100%;
    transition-duration: .1s;
    transition-property: transform, opacity;
    transition-timing-function: ease-out;
    transform: translate3d(0, 0, 0);
    opacity: 1;
}

.moveRight-leave.moveRight-leave-active {
    transform: translate3d(15%, 0, 0);
    opacity: 0.3;
}

.fadeIn-enter {
    opacity: 0.01;
    transition: opacity .2s ease-in;
}

.fadeIn-enter.fadeIn-enter-active {
    opacity: 1;
}

.fadeIn-leave {
    opacity: 1;
    transition: opacity .2s ease-in;
}

.fadeIn-leave.fadeIn-leave-active {
    opacity: 0.01;
}


.router{
    position:relative;
    &>div{
        width:100%;
        overflow:hidden;
        height:100%;
        // visibility:hidden;
        position:absolute;
        z-index:1000;
        background:#FFF;
        opacity:0.3;
        &.active{
            z-index:1001;
            opacity:1;
            // visibility:visible;
            transform:translate3d(0,0,0)
            animation:rightToLeft .3s ease-in;
        }
        &:first-child.active{
            animation:leftToRight .3s ease-in;            
        }
    }
}

@keyframes rightToLeft{
    from{
        transform:translate3d(100%,0,0)
    }
    to{
        transform:translate3d(0,0,0)
    }
}
@keyframes leftToRight{
    from{
        transform:translate3d(-100%,0,0)
    }
    to{
        transform:translate3d(0,0,0)
    }
}