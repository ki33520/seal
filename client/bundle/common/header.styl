.header {
    height: 1.17rem;
    line-height:1.17rem;
    background: url(/client/asset/images/headerBg.png) 50% 50% no-repeat;
    background-size: cover;
    text-align: center;
    position:relative;

    .title {
        font-size: .46rem;
    }
    .iconfont {
        position:absolute;
        left:0;
        top:0;
        z-index:9;
        font-size:.58rem;
        width:1.17rem;
        height:1.17rem;
        color:#999;
    }
    .btn-right {
        position:absolute;
        right:0;
        top:0;
        z-index:9;
        width:1.17rem;
        height:1.17rem;
        color:#801da1;
    }
    .btn-left {
        position: absolute;
        top: 0;
        left: 0;
        width: 1.17rem;
        height: 1.17rem;
        color: #666;
    }
}
